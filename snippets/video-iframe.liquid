{%- if video_object.type == 'vimeo' -%}
  {%- assign video_code = video_object.url | split: '.com/' -%}
  <iframe src="https://player.vimeo.com/video/{{ video_code[1] }}?autoplay=1&loop=1&autopause=0&muted=1&background=1" frameborder="0" width="100%" height="400px"></iframe>
{%- else -%}
  {%- if video_object.url != blank -%}
    {%- assign video_code = video_object.url | split: '=' -%}
  {%- else -%}
    {%- assign video_code = video_object | split: '=' -%}
  {%- endif -%}
  <iframe src="//www.youtube.com/embed/{{ video_code[1] }}?controls=1&showinfo=0&rel=0&autoplay=1&loop=1&playlist={{ video_code[1] }}&mute=1&controls=1&showinfo=0" frameborder="0" allowfullscreen width="100%" height="400px"></iframe>
{%- endif -%}
